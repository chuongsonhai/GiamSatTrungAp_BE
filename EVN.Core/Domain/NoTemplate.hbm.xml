<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping   xmlns="urn:nhibernate-mapping-2.2">
  <class name="EVN.Core.Domain.NoTemplate, EVN.Core" table="NoTemplate" lazy="true" >
    <id name="ID" column="ID">
		<generator class="native">
			<param name="sequence">NOTEMPLATE_ID_SEQ</param>
		</generator>
    </id>    
    <property name="CurrentNo" column="CurrentNo" type="Decimal"/>
    <property name="CurrentYear" column="CurrentYear" type="int"/>
    <property name="Format" column="Format" type="string"/>
    <property name="Status" column="Status" type="int"/>
    <property name="Type" column="Type"/>        
  </class>
</hibernate-mapping>