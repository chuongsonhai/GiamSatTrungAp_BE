<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping   xmlns="urn:nhibernate-mapping-2.2">
  <class name="EVN.Core.Domain.SystemConfig, EVN.Core" table="SystemConfig" lazy="true" >
    <id name="ID" column="ID">
		<generator class="native">
			<param name="sequence">SYSTEMCONFIG_ID_SEQ</param>
		</generator>
    </id>    
    <property name="Code">
      <column name="Code" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Value">
      <column name="Value" sql-type="nvarchar" not-null="false" />
    </property>

  </class>
</hibernate-mapping>